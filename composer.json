{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "artisaninweb/laravel-soap": "0.3.0.9",
        "barryvdh/laravel-dompdf": "^0.8.5",
        "botman/botman": "^2.5",
        "botman/driver-web": "^1.5",
        "cache/redis-adapter": "^1.0",
        "cache/simple-cache-bridge": "^1.0",
        "codedge/laravel-fpdf": "^1.0",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.4",
        "kylekatarnls/laravel-carbon-2": "^1.0.0",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.4.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "^3.1",
        "nesbot/carbon": "2.21.0 as 1.34.0",
        "nwidart/db-exporter": "^1.1",
        "phpoffice/phpspreadsheet": "^1.9",
        "setasign/fpdf": "^1.8",
        "superbalist/laravel-google-cloud-storage": "^2.2",
        "twilio/sdk": "^5.31",
        "vsmoraes/laravel-pdf": "^2.0",
        "chumper/zipper": "^1.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "files": ["app/Http/Helpers/Helpers.php","app/Http/Helpers/CssMinify.php","app/Http/Helpers/EncryptIdAlgorithm.php"],
    "minimum-stability": "dev",
    "prefer-stable": true
}
